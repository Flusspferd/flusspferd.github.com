<!-- ============================== constructor details ==================== -->		
			
			<div class="details"><a name="constructor"> </a>
				<div class="sectionTitle">
					Class Detail
				</div>
				
				<div class="fixedFont">
						<b>Import</b>(name, options)
				</div>
				
				<div class="description">
					<p>Load a module. Import is defined as a top level function on the global
object.</p>

					
				</div>
				
				
				
				<pre class="code">Import.paths.unshift('./lib');
Import('My.Module') // Will look for My/Module.js</pre>
				
				
				
				
					
						<dl class="detailList">
						<dt class="heading">Parameters:</dt>
						
							<dt>
								<span class="light fixedFont">{string}</span>  <b>name</b>
								
							</dt>
								<dd><p>Module to attempt to load</p>
</dd>
						
							<dt>
								<span class="light fixedFont">{object}</span>  <b>options</b>
								
							</dt>
								<dd><p>Additional options</p>
</dd>
						
						</dl>
					
					
					
					
					
					
					

